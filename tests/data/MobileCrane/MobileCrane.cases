{name        : 'MobileCrane',
description : 'Case Study with the Crane FMU and some other supporting FMUs',
modelFile : "OspSystemStructure.xml",
timeUnit  : "second",
variables : {
   p     : ['mobileCrane', 'pedestal_boom', "Length and angle of pedestal. Use p[2] to set pedestal azimuth [radians]"],
   b     : ['mobileCrane', 'boom_boom', "Length and angle of boom. Use b[1] to set boom polar angle [radians]"],
   da_dt : ['mobileCrane', 'craneAngularVelocity', "Angular velocity of whole crane with respect to base. [|da_dt|, normal]"],
   dp_dt : ['mobileCrane', 'pedestal_angularVelocity', "Angular velocity of pedestal (around z-axis). [radians]"],
   db_dt : ['mobileCrane', 'boom_angularVelocity', "Angular velocity of boom (polar direction). [radians]"],
   v : ['mobileCrane', 'craneVelocity', "Linear velocity of whole crane with respect to base. [|v|, normal]"],
   T  : ['mobileCrane', 'craneTorque', "Total torque of the crane with respect to its base, i.e. the sum of static and dynamic torques. Provided as 3D spherical vector."],
   load : ['mobileCrane', 'rope_tip', "Cartesian coordinate of rope tip (load position)"],   
},  
base : {
   description : "Variable settings for the base case. Represents a mobile crane ready for lifting (turned 90deg and boom lifted 45 deg)",
   spec: {
      stopTime : 10,
      stepSize : 0.1,
      p[2]  : 1.5708,
      b[1]  : '45 deg',
      da_dt : [0,0,0,0],
      dp_dt : 0,
      db_dt : 0,
      v     : [0,0,0,0],
   }},
results : {
   spec : [
      T@step,
      load@step,
   ]}
}
